{
  "private": true,
  "sideEffects": false,
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "npm run rm && npm run build:css && remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev": "dotenv -e .env.local -- concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "prisma:local:introspect": "dotenv -e .env.local -- npx prisma db pull",
    "prisma:local:migrate": "dotenv -e .env.local -- npx prisma migrate dev && npm run prisma:local:generate",
    "prisma:local:generate": "dotenv -e .env.local -- npx prisma generate",
    "prisma:local:seed": "dotenv -e .env.local -- npx prisma db seed",
    "prisma:prod:migrate": "dotenv -e .env -- npx prisma migrate deploy",
    "start": "npm run prisma:prod:migrate && remix-serve build",
    "start:local": "remix-serve build",
    "rm": "rm -rf ./build/*.* && rm -rf ./public/build/*.*"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.9",
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.5",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.5",
    "@fontsource/inter": "^4.5.11",
    "@paypal/react-paypal-js": "^7.8.1",
    "@prisma/client": "^4.6.0",
    "@react-icons/all-files": "^4.1.0",
    "@remix-run/node": "*",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.3",
    "framer-motion": "^6.5.1",
    "i18next": "^21.10.0",
    "i18next-browser-languagedetector": "^7.0.0",
    "i18next-fs-backend": "^2.0.0",
    "i18next-http-backend": "^2.0.1",
    "isbot": "^3.6.3",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-colorful": "^5.5.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.6",
    "remix-auth": "^3.3.0",
    "remix-auth-form": "^1.2.0",
    "remix-i18next": "^4.1.1",
    "scheduler": "^0.23.0",
    "swr": "^1.3.0",
    "use-context-selector": "^1.4.1",
    "use-debouncy": "^4.3.0"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/crypto-js": "^4.1.1",
    "@types/node": "^18.11.9",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.10.0",
    "husky": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "prisma": "^4.6.0",
    "tailwindcss": "^3.2.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=14"
  }
}
